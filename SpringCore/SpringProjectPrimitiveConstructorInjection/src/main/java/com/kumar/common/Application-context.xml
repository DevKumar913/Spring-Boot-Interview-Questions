<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean id="employee1" class ="com.kumar.model.Employee">
		<property name="name" value="Harish" ></property>
		<property name="age" value ="27"></property>
		<property name="salary" value="8000"></property>
		
	</bean>
	
	<bean id ="employee2" class="com.kumar.model.Employee">
	<constructor-arg name="name" value="Chary"> </constructor-arg>
	<constructor-arg name="age" value="29"> </constructor-arg>
	<constructor-arg name="salary" value="7000"> </constructor-arg>
	
	</bean>
</beans>
